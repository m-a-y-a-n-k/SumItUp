    "express-session": "^1.18.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "image-recognition": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.0.3",
    "mongoose": "^5.13.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.10.4",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
